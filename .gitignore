# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/public/*
/tmp/*
/shared/*
/shared/pids/puma.pid
/shared/pids/puma.state
!/log/.keep
!/tmp/.keep
/config/database.yml
.byebug_history
Gemfile.lock

